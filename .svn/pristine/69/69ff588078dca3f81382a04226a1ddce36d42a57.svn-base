.step_css ul li {
  float:left; 
  height:46px; 
  line-height:46px; 
  margin-right:9px; 
  margin-top:5px;
  padding-right:21px; 
  cursor:pointer; 
  color:#555;
  list-style: none;
}
.step_css ul .step_d {
}
.step_css ul .step_d .triangle {
  	width: 0;
	height: 0;
	border-style: solid;
	border-width: 23px 0 23px 21px;
	border-color: transparent transparent transparent #168B82;
	display:block;
	float:left;
  
}
.step_css ul .step_d .right_step {
  background: #168B82; 
  height:46px; 
  display:inline-block; 
  color:#FFF; 
  padding:0 8px; 
  _padding:0 8px 0 6px; 
  float:left;
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
.step_css ul .step_e {
}
.step_css ul .step_e .triangle {
