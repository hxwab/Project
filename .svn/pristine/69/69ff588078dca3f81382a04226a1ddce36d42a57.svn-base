	height: 0;
	border-style: solid;
	border-width: 23px 0 23px 21px;
	border-color: transparent transparent transparent #168B82;
	display:block;
	float:left;
	opacity:0.6;
  
}
.step_css ul .step_e .right_step {
  background: #168B82; 
