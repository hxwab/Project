<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="upload" extends="json-default"  namespace="/upload">
		<action name="resetGroup" class="uploadAction" method="resetGroup">
			<result type="json"><param name="root">json</param></result>
		</action>
		
		<action name="upload" class="uploadAction" method="upload">
			<result type="json"><param name="root">json</param></result>
		</action>
		
		<action name="discard" class="uploadAction" method="discard">
			<result type="json"><param name="root">json</param></result>
		</action>
		
		<action name="fetchGroupFilesSummary" class="uploadAction" method="fetchGroupFilesSummary">
			<result type="json"><param name="root">json</param></result>
		</action>
		

	</package>

</struts>